from typing import List

class Solution:
    def numPairsDivisibleBy60(self, time: List[int]) -> int:
        count = [0] * 60  # counts remainders
        res = 0
        for t in time:
            remainder = t % 60
            complement = (60 - remainder) % 60
            res += count[complement]  # add all previous matches
            count[remainder] += 1     # record this remainder
        return res
