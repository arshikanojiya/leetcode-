class Solution:
    def majorityElement(self, nums):
        """
        Return the element that appears more than floor(len(nums)/2) times.
        Boyerâ€“Moore voting algorithm.
        """
        count = 0
        candidate = None
        for num in nums:
            if count == 0:
                candidate = num
            count += 1 if num == candidate else -1
        return candidate

# quick local test
if __name__ == "__main__":
    print(Solution().majorityElement([3,2,3]))          # -> 3
    print(Solution().majorityElement([2,2,1,1,1,2,2]))  # -> 2
