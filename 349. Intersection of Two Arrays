class Solution:
    def intersection(self, nums1, nums2):
        set1 = set(nums1)
        set2 = set(nums2)
        return list(set1.intersection(set2))

# Now create an instance and call the method after the class is defined
param_1 = [1, 2, 2, 1]
param_2 = [2, 2]
ret = Solution().intersection(param_1, param_2)
print(ret)  # Output: [2]
